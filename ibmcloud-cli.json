    "homepage": "https://clis.ng.bluemix.net/ui/home.html",
    "license": "https://console.ng.bluemix.net/docs/navigation/notices.html",
    "architecture": {
        "64bit": {
            "url": "https://clis.ng.bluemix.net/download/bluemix-cli/0.9.0/win64#/dl.exe",
            "hash": "sha1:c4c4bd0f60f55a47f08083e3f0e3a75927bb3aad"
        },
        "32bit": {
            "url": "https://clis.ng.bluemix.net/download/bluemix-cli/0.9.0/win32#/dl.exe",
            "hash": "sha1:a86b0f6140e619adb011f535faf201a3c7e4027c"
        }
    },
    "bin": [
        "ibmcloud.exe",
        [
            "ibmcloud.exe",
            "bluemix"
        ],
        [
            "ibmcloud.exe",
            "bx"
        ],
        "ibmcloud_analytics.exe"
    ],
    "pre_install": [
        "pushd \"$dir\"",
        "# Error 1605 mitigation",
        "Start-Process \"$dir\\dl.exe\" -Wait -ErrorAction Stop -ArgumentList \"/s /x /b\"\"$dir\"\" /v\"\"/qn\"\"\" -WindowStyle Hidden",
        "extract_7zip \"$dir\\IBM_Cloud_CLI.msi\" \"$dir\\tmp\"",
        "extract_7zip \"$dir\\tmp\\Data1.cab\" \"$dir\"",
        "mkdir cfcli | out-null",
        "mv cf.exe cfcli",
        "rm -r tmp",
        "rm IBM_Cloud_CLI.msi",
        "rm dl.exe",
        "popd"
    ],
    "post_install": "bx plugin update --all",
    "checkver": {
        "github": "https://github.com/IBM-Bluemix/bluemix-cli-release"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://clis.ng.bluemix.net/download/bluemix-cli/$version/win64#/dl.exe"
            },
            "32bit": {
                "url": "https://clis.ng.bluemix.net/download/bluemix-cli/$version/win32#/dl.exe"
            }
        },
        "hash": {
            "url": "$url/checksum"
        }
    }
}
